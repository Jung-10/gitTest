a, b, c = map(int, input("세 변의 길이를 입력하세요 : ").split())

length = [a,b,c]

length.sort()

x = 1


while x:
    if (length[2] <= 0 or length[1] <= 0 or length[0] <= 0):
        print("양의 정수를 입력하세요.")
     
    elif (length[2] >= length[1] + length[0]) and (length[0] > 0 and length[1] > 0 and length[2] > 0):
        print(False)
    elif (length[2] < length[1] + length[0]) and (length[0] > 0 and length[1] > 0 and length[2] > 0):
        print(True)
    x = int(input("다시하시겠습니까? 예 = 1 아니오 = 0"))